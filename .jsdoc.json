{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["lib", "README.md"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "opts": {
    "destination": "./docs/",
    "readme": "./README.md",
    "encoding": "utf8",
    "private": true,
    "recurse": true
  }
}
