{
  "name": "@cocalc/local-storage-lru",
  "version": "2.4.3",
  "description": "Simple LRU cache for a browser's localStorage",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "test": "npm run lint && npm run jest && npm run make-badges",
    "test-ci": "npm run jest",
    "jest": "jest --config jestconfig.json",
    "clean": "rm -rf ./lib/ ./docs/ ./coverage/",
    "build": "npm run clean && tsc",
    "format": "prettier --write \"**/*.ts\" \"**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm test",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "make-badges": "istanbul-badges-readme",
    "docs-show": "xdg-open ./docs/index.html",
    "docs": "rm -rf ./docs/ && npx typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sagemathinc/local-storage-lru.git"
  },
  "keywords": [
    "localStorage",
    "LRU",
    "Cache"
  ],
  "author": "Sagemath, Inc.",
  "license": "APACHE-2.0",
  "homepage": "https://sagemathinc.github.io/local-storage-lru/",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "istanbul-badges-readme": "^1.8.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "files": [
    "lib/*",
    "lib/**/*"
  ],
  "bugs": {
    "url": "https://github.com/sagemathinc/local-storage-lru/issues"
  }
}
